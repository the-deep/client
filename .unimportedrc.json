{
  "entry": "src/index.js",
  "extensions": [".ts", ".js", ".tsx", ".jsx", ".scss"],
  "ignorePatterns": ["**/node_modules/**", "**/react-store/**", "**/redux/**", "**/vendor/**"],
  "aliases": {
    "#components/*": ["./src/components/*"],
    "#dui/*": ["./src/components/ui/*"],
    "#config/*": ["./src/config/*"],
    "#constants": ["./src/constants"],
    "#constants/*": ["./src/constants/*"],
    "#cs": ["./src/cs"],
    "#entities/*": ["./src/entities/*"],
    "#notify": ["./src/notify"],
    "#redux": ["./src/redux"],
    "#redux/*": ["./src/redux/*"],
    "#request": ["./src/request"],
    "#resources/*": ["./src/resources/*"],
    "#hooks/*": ["./src/hooks/*"],
    "#rest": ["./src/rest"],
    "#rest/*": ["./src/rest/*"],
    "#rsca/*": ["./src/vendor/react-store/components/Action/*"],
    "#rscg/*": ["./src/vendor/react-store/components/General/*"],
    "#rsci/*": ["./src/vendor/react-store/components/Input/*"],
    "#rscv/*": ["./src/vendor/react-store/components/View/*"],
    "#rscz/*": ["./src/vendor/react-store/components/Visualization/*"],
    "#rsk/*": ["./src/vendor/react-store/constants/*"],
    "#rsu/*": ["./src/vendor/react-store/utils/*"],
    "#schema": ["./src/schema"],
    "#store": ["./src/store"],
    "#store/*": ["./src/store/*"],
    "#theme": ["./src/theme"],
    "#ts": ["./src/ts"],
    "#utils/*": ["./src/utils/*"],
    "#views/*": ["./src/views/*"],
    "#widgetComponents/*": ["./src/widgetComponents/*"],
    "#widgets": ["./src/widgets"],
    "#widgets/*": ["./src/widgets/*"],
    "#typings": ["./src/typings"],
    "#typings/*": ["./src/typings/*"],
    "#qbc/*": ["./src/components/questionnaire-builder/*"]
  },
}
